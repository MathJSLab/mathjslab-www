{
  "title": "MathJSLab Project",
  "url": "https://mathjslab.com",
  "languages": {
    "en": {
      "home": "Home",
      "about": "About",
      "welcomeMessage": "Welcome to MathJSLab"
    },
    "pt": {
      "home": "In√≠cio",
      "about": "Sobre",
      "welcomeMessage": "Bem-vindo ao MathJSLab"
    },
    "es": {
      "home": "Inicio",
      "about": "Acerca de",
      "welcomeMessage": "Bienvenido a MathJSLab"
    }
  },
  "build": {
    /* Site project construction parameters. */
    "eleventy": {
      "commonOptions": {
        "htmlTemplateEngine": "njk",
        "markdownTemplateEngine": "njk",
        "dataTemplateEngine": "njk",
        "templateFormats": ["njk"]
      },
      "steps": [
        {
          "options": {
            "dir": {
              "input": "./site",
              "data": "../data",
              "includes": "../includes",
              "layouts": "./layouts",
              "output": "./www"
            },
            "pathPrefix": "/"
          }
        }
      ]
    }
  }
}
