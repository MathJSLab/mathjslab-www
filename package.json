{
  "name": "mathjslab-www",
  "version": "0.0.3",
  "description": "MathJSLab project homepage at mathjslab.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab-www.git"
  },
  "homepage": "https://mathjslab.com/",
  "author": "MathJSLab <mathjslab@gmail.com>",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "clean:package-lock": "node script/helper/clean-package-lock.cjs",
    "clean:script": "rimraf script/build",
    "clean:build": "rimraf www",
    "clean:script:build": "npm run clean:script && npm run clean:build",
    "clean": "npm run clean:script:build",
    "clean:download": "node script/helper/download-files.cjs clean && rimraf script/helper/download-files.cjs download.config.json",
    "clean:all": "npm run clean && npm run clean:package-lock && npm run clean:download",
    "download": "curl -k -o ./script/helper/download-files.cjs https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/script/helper/download-files.cjs && curl -k -o ./download.config.json https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/resource/mathjslab-www/download.config.json && node script/helper/download-files.cjs",
    "update": "npx npm-check-updates -u && npm install && npm run download && npm run clean && npm run build:script",
    "git:add": "git add .",
    "git:commit": "node script/helper/git-commit.cjs",
    "git:push": "git push",
    "git:all": "npm run git:add && npm run git:commit && npm run git:push",
    "build:script": "tsc -p script/tsconfig.json",
    "build": "npm run clean:build && node eleventy.build.mjs",
    "serve": "npm run eleventy --serve",
    "watch": "npm run eleventy --watch",
    "do:all": "npm run clean:all && npm run update && npm run build && npm run git:all"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.1",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/coffeescript": "^2.5.7",
    "@types/cson": "^7.20.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "chalk": "^5.4.1",
    "coffeescript": "^2.7.0",
    "cross-env": "^7.0.3",
    "cson": "^8.4.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "glob": "^11.0.1",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.9.2",
    "png-to-ico": "^2.1.8",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "smol-toml": "^1.3.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0",
    "yaml": "^2.7.0"
  }
}
