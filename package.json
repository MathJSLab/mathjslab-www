{
  "name": "mathjslab-www",
  "version": "0.0.1",
  "description": "MathJSLab project homepage at mathjslab.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab-www.git"
  },
  "homepage": "https://mathjslab.com/",
  "author": "MathJSLab <mathjslab@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "npx rimraf www",
    "clean:all": "npm run clean",
    "update": "echo \"Warning: no update script specified\"",
    "git:add": "git add .",
    "git:commit": "git commit -m \"Initial Commit\"",
    "git:push": "git push",
    "git:add:commit:push": "npm run git:add && npm run git:commit && npm run git:push",
    "git:all": "npm run git:add:commit:push",
    "build": "npx @11ty/eleventy",
    "serve": "npx @11ty/eleventy --serve",
    "watch": "npx @11ty/eleventy --watch",
    "do:all": "npm run clean && npm run update && npm run build && npm run git:add:commit:push"
  }
}
