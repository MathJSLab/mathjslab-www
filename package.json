{
  "name": "mathjslab-www",
  "version": "0.0.2",
  "description": "MathJSLab project homepage at mathjslab.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab-www.git"
  },
  "homepage": "https://mathjslab.com/",
  "author": "MathJSLab <mathjslab@gmail.com>",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "clean:package-lock": "node script/helper/clean-package-lock.cjs",
    "clean:build": "rimraf www",
    "clean": "npm run clean:build",
    "clean:download-resources": "node script/helper/download-files.cjs clean && rimraf script/helper/download-files.cjs download.config.json",
    "clean:all": "npm run clean && npm run clean:package-lock && npm run clean:download-resources",
    "download-resources": "curl -k -o ./script/helper/download-files.cjs https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/script/helper/download-files.cjs && curl -k -o ./download.config.json https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/resource/mathjslab-www/download.config.json && node script/helper/download-files.cjs",
    "update": "npx npm-check-updates -u && npm install && npm run download-resources",
    "git:add": "git add .",
    "git:commit": "git commit -m \"Initial Commit\"",
    "git:push": "git push",
    "git:all": "npm run git:add && npm run git:commit && npm run git:push",
    "build": "npm run clean:build && node --disable-warning=DEP0040 eleventy.build.mjs",
    "serve": "npm run eleventy --serve",
    "watch": "npm run eleventy --watch",
    "do:all": "npm run clean && npm run update && npm run build && npm run git:all"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^5.0.0",
    "chalk": "^5.3.0",
    "coffeescript": "^2.7.0",
    "cson": "^8.4.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.9.2",
    "png-to-ico": "^2.1.8",
    "rimraf": "^6.0.1",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "smol-toml": "^1.3.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "uuid": "^11.0.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "yaml": "^2.6.1"
  }
}
